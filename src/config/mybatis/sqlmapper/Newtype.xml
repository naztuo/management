<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.dcc.dao.Newtype">
<select id="selectType" resultType="NewsType">
	SELECT * FROM newtype
</select>
<insert id="addType" parameterType="NewsType">
INSERT INTO newtype (names,dates) VALUES (#{names},#{dates})
</insert>
</mapper>